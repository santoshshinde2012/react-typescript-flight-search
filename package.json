{
  "name": "react-typescript-flight-search",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/react-datepicker": "^2.0.1",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "enzyme-adapter-react-16": "^1.7.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react-addons-test-utils": "^15.6.2",
    "react-scripts-ts": "^4.0.8",
    "tslint": "^5.11.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-select": "^2.0.10",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "enzyme": "^3.8.0",
    "fs": "0.0.2",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-datepicker": "^2.0.0",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.2",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path src src/styles/*.scss public/global/css/index.css --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar --watch --include-path src src/styles/*.scss  public/global/css/index.css --recursive",
    "start-js": "react-scripts-ts start",
    "lint": "tslint --project tsconfig.json",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
